{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate to setup a react project",
  "main": "dist/main.js",
  "scripts": {
    "start": "webpack serve --hot --mode development --config webpack.config.js",
    "build": "webpack --mode production",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "test": "jest",
    "test:ci": "npm-run-all tscompile lint test",
    "tscompile": "tsc --noEmit"
  },
  "author": "Joke Frerichs",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/jest": "26.0.24",
    "@types/react": "17.0.45",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.7.2",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "styled-jsx": "5.0.2",
    "@webpack-cli/serve": "1.6.1",
    "babel-loader": "8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "html-webpack-plugin": "^5.5.0",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3"
  },
  "resolutions": {
    "react-devtools-core": "4.14.0"
  },
  "jest": {
    "verbose": false,
    "moduleNameMapper": {
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
